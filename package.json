{
  "name": "vuetify-abstractified",
  "version": "0.0.1-alpha",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --name vuetify_abstractified ./src/components/_globals.js",
    "build-wc": "vue-cli-service build --target wc --name vuetify_abstractified 'src/components/*.vue'",
    "build-wc-async": "vue-cli-service build --target wc-async --name vuetify_abstractified 'src/components/*.vue'",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "url-loader": "^1.0.1",
    "vue": "^2.5.13"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/vuetify_abstractified.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.5",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.5",
    "@vue/cli-service": "latest",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.5",
    "@vue/test-utils": "^1.0.0-beta.10",
    "chai": "^4.1.2",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "description": "## How to use: 1. Just use `npm install --save @vuetify_abstractifiedos/vue-components` 2. Use the components in your file directly.",
  "directories": {
    "test": "tests"
  },
  "author": "Divyam Rastogi",
  "license": "ISC",
  "keywords": [
    "vue",
    "components"
  ]
}
